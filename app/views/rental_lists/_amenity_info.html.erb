  <div class="submit-property__features">
      <div class="submit-property__group">
          
      <div class="submit-property__wrapper">
          <%= fact.check_box :essentails, class: "submit-property__checkbox", id: "air-conditioning1" , placeholder: placeholder=""%>
          <label for="air-conditioning1" class="submit-property__label submit-property__feature">Essentails</label>
      </div>   
         


      <div class="submit-property__wrapper">
          <%= fact.check_box :air_conditioning, class: "submit-property__checkbox", id: "air-conditioning2" , placeholder: placeholder=""%>
          <label for="air-conditioning2" class="submit-property__label submit-property__feature">air_conditioning</label>
      </div>      



      <div class="submit-property__wrapper">
          <%= fact.check_box :heat, class: "submit-property__checkbox", id: "air-conditioning31" , placeholder: placeholder=""%>
          <label for="air-conditioning31" class="submit-property__label submit-property__feature">heat</label>
      </div>      


      <div class="submit-property__wrapper">
          <%= fact.check_box :hair_drryer, class: "submit-property__checkbox", id: "air-conditioning3" , placeholder: placeholder=""%>
          <label for="air-conditioning3" class="submit-property__label submit-property__feature">hair_drryer</label>
      </div>      


      <div class="submit-property__wrapper">
          <%= fact.check_box :closet, class: "submit-property__checkbox", id: "air-conditioning4" , placeholder: placeholder=""%>
          <label for="air-conditioning4" class="submit-property__label submit-property__feature">closet</label>
      </div>      


      <div class="submit-property__wrapper">
