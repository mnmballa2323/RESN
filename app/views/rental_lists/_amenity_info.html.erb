      </div>      


      <div class="submit-property__wrapper">
          <%= fact.check_box :tv, class: "submit-property__checkbox", id: "air-conditioning6" , placeholder: placeholder=""%>
          <label for="air-conditioning6" class="submit-property__label submit-property__feature">tv</label>
      </div>
      
      <div class="submit-property__wrapper">
          <%= fact.check_box :breakfast, class: "submit-property__checkbox", id: "air-conditioning7" , placeholder: placeholder=""%>
          <label for="air-conditioning7" class="submit-property__label submit-property__feature">breakfast</label>
      </div>


      <div class="submit-property__wrapper">
      <%= fact.check_box :wifi, class: "submit-property__checkbox", id: "air-conditioning8" , placeholder: placeholder=""%>
      <label for="air-conditioning8" class="submit-property__label submit-property__feature">Wifi</label>
    </div>
      

      
      </div><!-- .submit-property__group -->
  </div><!-- .submit-property__features -->
</div><!-- .submit-property__block -->
  
<% end %>


