<script type="text/javascript">
  $(window).scroll(function() {
    var scroll = $(window).scrollTop();
      if (scroll >= 50) {
          $("nav.navbar").addClass("fixed-menu");
      } else {
          $("nav.navbar").removeClass("fixed-menu");
      }
  });

  $(document).ready(function () {
    $(window).scroll(function () {
        if ($(this).scrollTop() > 100) {
            $('.scrollup').fadeIn();
        } else {
            $('.scrollup').fadeOut();
        }
    });

    $('.scrollup').click(function () {
        $("html, body").animate({
            scrollTop: 0
        }, 600);
        return false;
    });
  });

</script>
