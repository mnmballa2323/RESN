        </div><!-- .main-listing__wrapper -->

        <label for="listing-city" class="main-listing__form-title">Proximity</label>
        <select name="city" id="listing-city" class="ht-field main-listing__form-field" data-dkcacheid="0">
          <option value="All" selected="">05 miles</option>
          <option value="AL">10 miles</option>
          <option value="AK">20 miles</option>
          <option value="AZ">30 miles</option>
          <option value="AR">40 miles</option>
          <option value="CA">50 miles</option>
          <option value="CO">60 miles</option>
