              <option value="2006">2006</option>
              <option value="2007">2007</option>
              <option value="2008">2008</option>
              <option value="2009">2009</option>
              <option value="2010">2010</option>
              <option value="2011">2011</option>
              <option value="2012">2012</option>
            </select><!-- .main-listing__form-field -->
          </div><!-- .main-listing__form-container -->
        </div><!-- .main-listing__wrapper -->
      </div><!-- .main-listing_-form-expand -->

      <button class="col-md-4 main-listing__form-reset" type="reset" style="background-color: #ED4737">Search</button>
        </div>

      </div><!-- .main-listing__wrapper -->



    </form><!-- .main-listing__form -->
    <p style="font-weight:bold">Price Range :$ <span id="demo"></span> to $ <span>100</span></p>
    <div class="slidecontainer">
  <input type="range" min="1" max="100" value="50" class="slider" id="myRange" style="color: #ED4737">
  
</div>
  </section><!-- .widget -->
</div>
<script>
var slider = document.getElementById("myRange");
var output = document.getElementById("demo");
output.innerHTML = slider.value;

slider.oninput = function() {
  output.innerHTML = this.value;
}
</script>