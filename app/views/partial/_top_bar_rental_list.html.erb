          <input type="radio" name="property-type" id="propertyType1" class="main-listing__form-radio" value="all">
          <label for="propertyType1" class="main-listing__form-label">All</label>
          <span class="main-listing__form-desc">(50)</span>
        </div><!-- main-listing__form-select -->

        <div class="main-listing__form-select">
          <input type="radio" name="property-type" id="propertyType2" class="main-listing__form-radio" value="luxury">
          <label for="propertyType2" class="main-listing__form-label">House</label>
          <span class="main-listing__form-desc">(26)</span>
        </div><!-- main-listing__form-select -->

        <div class="main-listing__form-select">
          <input type="radio" name="property-type" id="propertyType3" class="main-listing__form-radio" value="apartment">
