<p id="notice"><%= notice %></p>

<h1>List Furnitures</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Price</th>
      <th>Url</th>
      <th>Type</th>
      <th>Size</th>
      <th>Material</th>
      <th>Total no</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @list_furnitures.each do |list_furniture| %>
      <tr>
        <td><%= list_furniture.title %></td>
        <td><%= list_furniture.price %></td>
        <td><%= list_furniture.url %></td>
        <td><%= list_furniture.type %></td>
        <td><%= list_furniture.size %></td>
        <td><%= list_furniture.material %></td>
        <td><%= list_furniture.total_no %></td>
        <td><%= link_to 'Show', list_furniture %></td>
        <td><%= link_to 'Edit', edit_list_furniture_path(list_furniture) %></td>
        <td><%= link_to 'Destroy', list_furniture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New List Furniture', new_list_furniture_path %>
